{
  "version": 2,
  "builds": [
    {
      "src": "api/scrape.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape",
      "dest": "/api/scrape.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/scrape.py": {
      "maxDuration": 60
    }
  }
}
